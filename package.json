{
  "name": "AsyncMeeting",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev-preinstall": "npm install -g node-gyp",
    "dev-postinstall": "./node_modules/.bin/init-pg-testing-utils.js && bower install --config.interactive=false && ./node_modules/grunt-protractor-runner/node_modules/protractor/bin/webdriver-manager update && ./node_modules/grunt-cli/bin/grunt post-install && npm run make-redis",
    "make-redis": "cd ./node_modules/redis-install/redis-3.0.2 && make && cd ../../../",
    "pretest": "npm run-script lint && ./node_modules/grunt-cli/bin/grunt start-external-services",
    "test": "npm run unit-tests:client && npm run unit-tests:server && npm run integration-tests:client-only && npm run integration-tests:server-only",
    "posttest": "./node_modules/.bin/istanbul check-coverage && ./node_modules/grunt-cli/bin/grunt kill-external-services",
    "unit-tests:client": "./node_modules/grunt-cli/bin/grunt karma:test/unit/ng/karma.conf.js ",
    "unit-tests:server": "./node_modules/grunt-cli/bin/grunt istanbul-mocha:test/unit/server",
    "unit-tests:server-only": "./node_modules/grunt-cli/bin/grunt mocha:test/unit/server",
    "integration-tests:client": "./node_modules/grunt-cli/bin/grunt karma:test/integration/ng/karma.conf.js",
    "integration-tests:server": "./node_modules/grunt-cli/bin/grunt mocha:test/integration/server",
    "integration-tests:client-only": "./node_modules/grunt-cli/bin/grunt karma:test/integration/ng/karma.conf.js",
    "integration-tests:server-only": "./node_modules/grunt-cli/bin/grunt mocha:test/integration/server",
    "e2e": "./node_modules/grunt-cli/bin/grunt e2e:::~@WIP ",
    "e2e-all": "./node_modules/grunt-cli/bin/grunt e2e ",
    "lint": "./node_modules/.bin/jshint .",
    "checkStyle": "./node_modules/.bin/jscs .",
    "dev": "./node_modules/grunt-cli/bin/grunt start-external-services && node ./support/npm/launch-dev.js",
    "start-external-services": "./node_modules/grunt-cli/bin/grunt start-external-services",
    "kill-external-services": "./node_modules/grunt-cli/bin/grunt kill-external-services",
    "docker-dev-setup": "npm run docker-dev-cleanup-all && docker pull redis && docker pull mongo && docker run --name async-redis -d redis && docker run -p 27017:27017 --name async-mongo -d mongo",
    "docker-dev-build": "gulp build && docker build -t productivegains/asyncmeeting:dev .",
    "docker-dev-run": "docker kill async | echo '' && docker rm async | echo '' && docker run -d -p 3000:3000 --link async-redis:redis --link async-mongo:mongo --name async productivegains/asyncmeeting:dev",
    "docker-dev-cleanup-all": "docker kill async async-redis async-mongo | echo '' && docker rm async async-redis async-mongo | echo ''",
    "docker-dev-e2e": "./node_modules/grunt-cli/bin/grunt e2e:::~@WIP:true ",
    "docker-dev-integration": "./node_modules/grunt-cli/bin/grunt mocha:test/integration/server:true "
  },
  "dependencies": {
    "acl": "git+https://github.com/OptimalBits/node_acl.git",
    "body-parser": "^1.14.1",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^0.7.0",
    "connect-redis": "^2.5.1",
    "cookie-parser": "^1.4.0",
    "debug": "^2.1.3",
    "easy-session": "^0.1.3",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "fs.extra": "^1.3.2",
    "glob": "^5.0.15",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-livereload": "^3.8.1",
    "gulp-ng-annotate": "^0.5.3",
    "gulp-nodemon": "^2.0.4",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylus": "^2.1.1",
    "gulp-uglify": "^1.5.1",
    "install": "^0.1.8",
    "jade": "^1.11.0",
    "jsonwebtoken": "^5.4.1",
    "load-grunt-tasks": "^3.3.0",
    "lodash": "^3.10.1",
    "mongodb": "^2.0.49",
    "mongoose": "^4.2.9",
    "morgan": "^1.6.1",
    "nconf": "^0.7.2",
    "nconf-redis": "^0.3.1",
    "node-fs": "^0.1.7",
    "node-uuid": "^1.4.7",
    "npm": "^2.14.14",
    "passport": "^0.2.2",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "proxyquire": "^1.7.3",
    "q": "^1.4.1",
    "redis": "^0.12.1",
    "request-ip": "^1.1.4",
    "scrypt": "^4.0.7",
    "serve-favicon": "^2.3.0",
    "winston": "^0.9.0",
    "ws": "^0.7.2"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "chai": "^2.1.2",
    "chai-as-promised": "^4.3.0",
    "cucumber": "^0.4.8",
    "data-driven": "^1.2.0",
    "easyimage": "^1.3.0",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-gulp": "^0.1.0",
    "grunt-protractor-runner": "git+https://github.com/bailejl/grunt-protractor-runner.git",
    "istanbul": "^0.3.14",
    "jscs": "^1.13.1",
    "jshint": "^2.6.3",
    "karma": "^0.12.31",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.1.12",
    "karma-coverage": "^0.3.1",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.1.5",
    "karma-mocha": "^0.1.10",
    "karma-safari-launcher": "^0.1.1",
    "karma-sinon": "^1.0.4",
    "karma-sinon-chai": "^0.3.0",
    "mocha": "^2.2.1",
    "mockery": "^1.4.0",
    "nock": "^2.2.0",
    "pg-testing-utils": "git+ssh://git@github.com:Productive-Gains/Testing-Tools.git",
    "rewire": "^2.3.3",
    "rimraf": "^2.3.4",
    "selenium-webdriver": "2.45.1",
    "should": "^6.0.3",
    "sinon": "^1.14.1",
    "sleep": "^3.0.0",
    "supertest": "^0.15.0"
  }
}
